{
    "terminal.integrated.profiles.linux": {
        "zsh-shell": {
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/source.zsh"
            ]
        },
        "meterpreter-handler": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/source_metasploit.zsh"
            ]
        },
        "netcat-handler": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/revshell.zsh"
            ]
        },
        "web-delivery": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/source_web-delivery.zsh"
            ]
        }
    },
    "terminal.integrated.profiles.osx": {
        "zsh-shell": {
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/source.zsh"
            ]
        },
        "meterpreter-handler": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/source_metasploit.zsh"
            ]
        },
        "netcat-handler": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/revshell.zsh"
            ]
        },
        "web-delivery": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "source ${workspaceFolder}/.vscode/source_web-delivery.zsh"
            ]
        },
        "kali-orbstack": {
            "overrideName": true,
            "env": {
                "PROJECT_FOLDER": "${workspaceFolder}"
            },
            "path": "zsh",
            "args": [
                "-c",
                "orb -u root source ${workspaceFolder}/.vscode/source_kali-orbstack.zsh"
            ]
        }
    },
    "terminal.integrated.defaultProfile.osx": "zsh-shell",
    "terminal.integrated.defaultProfile.linux": "zsh-shell",
}